@model IEnumerable<OnlineVotingSystemForCollege.Models.ViewModel.ResultViewModel>

@{
    ViewBag.Title = "ListResult";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section breadcumb1
{
    <ol class="breadcrumb">
        <li>
            <a href="#">
                <em class="fa fa-home"></em>
            </a>
        </li>
        <li class="active">Result</li>
    </ol>
}
@section breadcumb2
{
    <div class="col-lg-12">
        <h1 class="page-header">View Result</h1>
    </div>
}

<div class="panel panel-default">
  
    <div class="panel-body">


        <table class="table">
            <tr>
              
                <th>
                    @Html.DisplayNameFor(model => model.CandidateName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Photo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ElectionName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.VoteObtained)
                </th>
             
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                  
                    <td>
                        @Html.DisplayFor(modelItem => item.CandidateName)
                    </td>
                    <td>
                        <img src="~/CandidateImages/@item.Photo" alt="" height=100 width=200 />
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ElectionName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.VoteObtained)
                    </td>
             
                </tr>
            }

        </table>
        </div>
    </div>
